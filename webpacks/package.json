		"jquery"                           : "./node_modules/jquery/dist/jquery.js",
		"vue"                              : "./node_modules/vue/dist/vue.js",
		"q"                                : "./node_modules/q/q.js",
		"velocity"                         : "./node_modules/velocity-animate/velocity.js",
		"velocity.ui"                      : "./node_modules/velocity-animate/velocity.ui.js",
		"jquery.scrollTo"                  : "./node_modules/jquery.scrollto/jquery.scrollTo.js",
		"jquery.extended"                  : "./private/jquery-plugins/src/jquery.extended.js",
		"jquery.modality"                  : "./private/jquery-plugins/src/jquery.modality.js",
		"jquery.modality.dialog"           : "./private/jquery-plugins/src/jquery.modality.dialog.js",
		"jquery.formValidation"            : "./private/form-validation/js/formValidation.popular.min.js",
		"jquery.formValidation.foundation" : "./private/form-validation/js/framework/foundation.js"
	},
	"browserify-shim": {
		"lodash"     : "_",
		"js-cookie"  : "Cookies",
		"knockout"   : "ko",
		"vue"        : "Vue",
		"q"          : "Q",
		"jquery"     : "jQuery"
	}
}
