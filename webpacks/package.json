	"main"        : "gulpfile.js",
	"devDependencies": {
		"browserify"          : "^11.2.0",
		"browserify-shim"     : "^3.8.10",
		"gulp"                : "^3.9.0",
		"gulp-util"           : "^3.0.6",
		"gulp-watch"          : "^4.3.3",
		"lodash.assign"       : "^3.2.0",
		"vinyl-buffer"        : "^1.0.0",
		"vinyl-source-stream" : "^1.1.0",
		"watchify"            : "^3.3.0",
		"yuidoc-lucid-theme"  : "^0.1.1"
	},
	"dependencies": {
		"html5shiv": "^3.7.2",
		"fastclick": "^1.0.6",
		"lodash": "^4.6.1",
		"js-cookie": "^2.1.0",
		"jquery": "^2.2.2",
		"vue": "^1.0.20",
		"q": "1.4.1",
		"velocity-animate": "1.2.3",
		"jquery.scrollto":"2.1.2"
	},
	"browserify": {
		"transform": [
			"browserify-shim"
		]
	},
	"browser": {
		"html5shiv"                        : "./node_modules/html5shiv/dist/html5shiv.js",
		"fastclick"                        : "./node_modules/fastclick/lib/fastclick.js",
		"lodash"                           : "./node_modules/lodash/lodash.js",
