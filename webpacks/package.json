	"main"        : "gulpfile.babel.js",
	"devDependencies": {
		"babel-core"          : "^6.3.26",
  		"babel-preset-es2015" : "^6.3.13",
		"browserify"          : "^11.2.0",
		"browserify-shim"     : "^3.8.10",
		"babelify"			  : "^7.3.0",
		"gulp"                : "^3.9.0",
		"gulp-cli"            : "^1.1.0",
		"gulp-util"           : "^3.0.6",
		"gulp-watch"          : "^4.3.3",
		"lodash.assign"       : "^3.2.0",
		"vinyl-source-stream" : "^1.1.0",
