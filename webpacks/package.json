		"yuidoc-lucid-theme"  : "^0.1.1"
	},
	"dependencies": {
		"html5shiv"        : "^3.7.2",
		"fastclick"        : "^1.0.6",
		"lodash"           : "^4.11.0",
		"js-cookie"        : "^2.1.0",
		"jquery"           : "^2.2.2",
		"vue"              : "^1.0.20",
		"bluebird"         : "^3.4.0",
		"velocity-animate" : "^1.2.3",
		"jquery.scrollto"  : "^2.1.2"
	},
	"browserify": {
		"transform": [
			"browserify-shim"
		]
	},
	"browser": {
        "html5shiv"       : "./node_modules/html5shiv/dist/html5shiv.js",
        "fastclick"       : "./node_modules/fastclick/lib/fastclick.js",
        "lodash"          : "./node_modules/lodash/lodash.js",
        "jquery"          : "./node_modules/jquery/dist/jquery.js",
        "vue"             : "./node_modules/vue/dist/vue.js",
        "bluebird"        : "./node_modules/bluebird/js/browser/bluebird.core.js",
        "velocity"        : "./node_modules/velocity-animate/velocity.js",
        "velocity.ui"     : "./node_modules/velocity-animate/velocity.ui.js",
		"js-cookie"       : "./node_modules/js-cookie/src/js.cookie.js",
        "jquery.scrollTo" : "./node_modules/jquery.scrollto/jquery.scrollTo.js"
	},
	"browserify-shim": {
		"lodash"     : "_",
		"vue"        : "Vue",
		"bluebird"   : "Promise",
		"jquery"     : "jQuery",
		"js-cookie"  : "Cookies"
